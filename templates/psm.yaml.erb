collector:
  psm:
    all:
      account_key: <%= account_key %>
        <% if environment %>
      environment: <%= environment %>
        <% end %>
        <% if hostname %>
      hostname: <%= hostname %>
        <% end %>
      agent_ruby_bin: <%= agent_ruby_bin %>
        <% if roles %>
      roles: <%= roles %>
        <% end %>

      plugin_directory: /opt/SolarWinds/Snap/bin/psm

load:
  plugin: snap-plugin-collector-psm
  task: task-psm.yaml
