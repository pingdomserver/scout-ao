collector:
  psm:
    all:
      account_key: <%= account_key %>
      <% if environment %>
      environment: <%= environment %>
      <% end %>
      <% if hostname %>
      hostname: <%= hostname %>
      <% end %>
      agent_ruby_bin: <%= agent_ruby_bin %>
      <% if roles %>
      roles: <%= roles %>
      <% end %>
      <% if key_processes.any? %>
      key_processes: <%= key_processes.join(',') %>
      <% end %>
      plugin_directory: /opt/appoptics/opt/psm

load:
  plugin: snap-plugin-collector-psm
  task: task-psm.yaml
